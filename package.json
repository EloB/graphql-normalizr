{
  "name": "graphql-normalizr",
  "version": "2.1.3",
  "description": "Normalize GraphQL response",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/monojack/graphql-normalizr.git"
  },
  "keywords": [
    "javascript",
    "graphql",
    "normalizr",
    "normalize"
  ],
  "author": "Ionut Achim <achimvionut@gmail.com>",
  "license": "MIT",
  "main": "lib/index.js",
  "module": "es/index.js",
  "dependencies": {
    "humps": "2.0.1",
    "pluralize": "7.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.1.2",
    "@babel/core": "7.1.2",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0",
    "@babel/preset-env": "7.1.0",
    "ava": "0.25.0",
    "eslint": "5.6.1",
    "eslint-config-mono": "2.0.0",
    "graphql": "14.0.2",
    "graphql-tag": "2.10.0",
    "rollup": "0.66.4",
    "rollup-plugin-babel": "4.0.3",
    "rollup-plugin-node-resolve": "3.4.0",
    "rollup-plugin-replace": "2.0.0",
    "rollup-plugin-size-snapshot": "0.7.0",
    "rollup-plugin-uglify": "6.0.0"
  },
  "bugs": {
    "url": "https://github.com/monojack/graphql-normalizr/issues"
  },
  "peerDependencies": {
    "graphql": "^0.11.* || ^0.12.* || ^0.13.* || ^0.14.*"
  },
  "scripts": {
    "dev": "npm run build:es -- -w",
    "pretest": "npm run build:cjs",
    "test": "ava --verbose --serial",
    "build:es": "BABEL_ENV=es babel src --out-dir es",
    "build:cjs": "BABEL_ENV=cjs babel src --out-dir lib",
    "build": "npm run build:cjs && npm run build:es",
    "prepare": "npm run clean && npm run build",
    "clean": "rimraf lib es"
  },
  "ava": {
    "files": [
      "test/*.js",
      "!test/mocks/**"
    ]
  },
  "npmName": "graphql-normalizr",
  "files": [
    "lib",
    "es",
    "src"
  ]
}
